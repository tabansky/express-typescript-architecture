{
  "name": "express-typescript-architecture",
  "version": "0.0.2",
  "description": "Configured express with typescript",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run build && node build/index.js",
    "dev": "nodemon",
    "test": "jest",
    "build": "rm -rf build && tsc && tsc-alias",
    "lint": "eslint . --ext=.ts",
    "lint:fix": "eslint . --ext=.ts --fix",
    "seed": "knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts --env development",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts --env development",
    "types": "shx rm -rf src/types/joi && ts-node lib/generate-types.ts"
  },
  "author": "tabansky",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.11.0",
    "knex": "^3.1.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/hpp": "^0.2.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "joi-to-typescript": "^4.11.0",
    "nodemon": "^3.0.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "typescript": "~5.2"
  }
}
